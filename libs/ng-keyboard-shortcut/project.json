{
  "projectType": "library",
  "sourceRoot": "libs/ng-keyboard-shortcut/src",
  "prefix": "ng-keyboard-shortcuts",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": [
        "dist/libs/ng-keyboard-shortcut"
      ],
      "options": {
        "project": "libs/ng-keyboard-shortcut/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ng-keyboard-shortcut/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ng-keyboard-shortcut/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/ng-keyboard-shortcut"
      ],
      "options": {
        "jestConfig": "libs/ng-keyboard-shortcut/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ng-keyboard-shortcut/**/*.ts",
          "libs/ng-keyboard-shortcut/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
