{
  "name": "ng-keyboard-shortcuts-app",
  "version": "8.0.0",
  "description": "Dead Simple Keyboard Shortcuts Management for Angular 2-7",
  "keywords": [
    "angular",
    "keyboard",
    "shortcuts",
    "keyboard-shortcuts",
    "rxjs"
  ],
  "license": "MIT",
  "author": "Omri Katz <omrikatz12@gmail.com> (https://github.com/omridevk)",
  "homepage": "https://omridevk.github.io/ng-keyboard-shortcuts/",
  "repository": "github.com/omridevk/ng-keyboard-shortcuts",
  "bugs": "https://github.com/omridevk/ng-keyboard-shortcuts/issues",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=0.0.0.0",
    "build": "ng build",
    "test": "ng test",
    "clean": "rm -rf dist/",
    "npm-pack": "cd dist/ng-keyboard-shortcuts && npm pack",
    "compodoc": "npx compodoc -p projects/ng-keyboard-shortcuts/tsconfig.lib.json --disableLifeCycleHooks --hideGenerator -d ./docs --disablePrivate",
    "compodoc-serve": "npx compodoc -p projects/ng-keyboard-shortcuts/tsconfig.lib.json --disableLifeCycleHooks --hideGenerator -s -d ./docs --disablePrivate -w",
    "build:prod": "npm run clean && npm run copy && ng build --prod ng-keyboard-shortcuts",
    "copy": "node ./scripts/copy.js",
    "build:watch": "npm run clean && ng build ng-keyboard-shortcuts --watch",
    "publish": "npm run build:prod && npm publish dist/ng-keyboard-shortcuts",
    "package": "npm run build:prod && npm run npm-pack",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^8.0.2",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.2",
    "@angular/compiler": "^8.1.0-next.3",
    "@angular/core": "^8.0.2",
    "@angular/forms": "^8.0.2",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.2",
    "@angular/platform-browser-dynamic": "^8.0.2",
    "@angular/router": "^8.0.2",
    "core-js": "^2.5.4",
    "prettier": "^1.16.4",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular/cli": "~8.0.3",
    "@angular/compiler-cli": "^8.1.0-next.3",
    "@angular/language-service": "^8.0.2",
    "@compodoc/compodoc": "1.1.8",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.3.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "ts-node": "~5.0.1",
    "tsickle": "^0.35.0",
    "tslib": "^1.10.0",
    "tslint": "~5.9.1",
    "typescript": "~3.4.5"
  }
}
